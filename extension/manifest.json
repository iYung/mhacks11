{
    "manifest_version": 2,
    "name": "MHacks11",
    "version": "0.0.0.1",
    "browser_action": {
      "default_icon": "icons/icon.png",
      "default_title": "MHacks11",
      "default_popup": "popup/main.html"
    },
    "content_scripts": [
      {
        "matches": ["*://*.reddit.com/*"],
        "js": ["main.js"]
      }
    ],
    "icons": {
      "48": "icons/icon.png"
    },
    "permissions": ["storage"],
    "description": "Be nice!",
    "background": {
        "scripts": ["background.js"]
    }
  }